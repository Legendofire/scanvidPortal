                <script>
                  $("#add-project-button").click(function(){
                    fetchSerial();
                    $(".stepContainer").height(220);
                    $('.buttonFinish').click(function(){
                      submitForm();
                    });
                  });
                  function fetchSerial(){
                    $.ajax({
                       type: "POST",
                       url: "/getSerial",
                       dataType: "json",
                       success: function(val){
                         $("#serial").val(val);
                       }
                    });
                  }
                  function submitForm(){
                    var data = {
                      serial: $("#serial").val(),
                      name: $("#name").val(),
                      description: $("#description").val(),
                      owningEntity: $("#owningEntity").val(),
                      projectExec: $("#projectExec").val(),
                      indecies: [],
                      plannedBudget: $("#plannedBudget").val(),
                      plannedDuration: $("#plannedDuration").val(),
                      startDate: $("#startDate").val()
                    };

                    var indecies = ["EOBD","GCI","CPI","WGI"];

                    for(var i=0; i<indecies.length; i++){
                      if($("#"+indecies[i]+"-check").prop("checked")){
                        var indexObj = {
                          name: indecies[i],
                          KPIs: []
                        };
                        $("#"+indecies[i]+"-kpis").find("input:checkbox").each(function(){
                          if($(this).prop("checked")){
                            var tempArr = $(this).attr("id").split("-");
                            tempArr[2] = "value";
                            var valName = "#"+tempArr.join('-');
                            indexObj.KPIs.push({
                              name: tempArr[1],
                              value: $(valName).val()
                            });
                          }
                        });
                        data.indecies.push(indexObj);
                      }
                    }

                    console.log(JSON.stringify(data));

                    $.ajax({
                       type: "POST",
                       url: "/add",
                       dataType: "json",
                       data: {data: JSON.stringify(data)},
                       beforeSend: function(){

                       },
                       success: function(){
                         window.location.href = window.location.href;
                       }
                    });
                  }

                  $("#EOBD-kpis").find("input").prop("disabled", true);
                  $("#GCI-kpis").find("input").prop("disabled", true);
                  $("#CPI-kpis").find("input").prop("disabled", true);
                  $("#WGI-kpis").find("input").prop("disabled", true);

                  $(".index-checkbox").click(function(){
                    var nameArr = $(this).attr("id").split("-");
                    nameArr = nameArr[0];
                    if($(this).prop("checked")){
                      $("#"+nameArr+"-kpis").find("input.kpi-checkbox").prop("disabled", false);
                    }else{
                      $("#"+nameArr+"-kpis").find("input.kpi-checkbox").prop("disabled", true);
                    }
                  });

                  $(".kpi-checkbox").click(function(){
                    var tempArr = $(this).attr("id").split("-");
                    tempArr[2] = "value";
                    var target_id = "#"+tempArr.join("-");
                    if($(this).prop("checked")){
                      $(target_id).prop("disabled", false);
                    }else{
                      $(target_id).prop("disabled", true);
                    }
                  });
                </script>
