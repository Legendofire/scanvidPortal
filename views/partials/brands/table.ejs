<!-- page content -->
<div class="right_col" role="main">
  <div class="">
    <div class="page-title">
      <div class="title_left">
        <h3>Brands <small>View, Edit and Delete brands!</small></h3>
      </div>
    </div>
  </div>

  <div class="clearfix"></div>
  <div class="row">
    <div class="col-md-12">
      <div class="x_panel">
        <div class="x_title">
          <h2>Brands</h2>
          <div class="nav navbar-right panel_toolbox">

          </div>
          <div class="clearfix"></div>
        </div>
        <div class="x_content">

          <!-- start project list -->
          <table class="table table-striped projects">
            <thead>
              <tr>
                <thead>
                  <tr>
                    <th>#</th>
                    <th>Brand Name</th>
                    <th>Actions</th>
                  </tr>
                </thead>
              </tr>
            </thead>
            <tbody>
              <% for(var i=0; i<brands.length; i++){ %>
              <tr>
                <th scope="row">
                  <%= i %>
                </th>
                <td>
                  <%= brands[i].brandName %>
                </td>
                <td>
                  <a href="/brands/view/<%= brands[i]._id %>" class="btn btn-primary btn-xs"><i class="fa fa-folder"></i> View </a>
                  <a href="#" data-info="<%=brands[i]._id%>,brands" class="btn btn-danger btn-xs delete_button"><i class="fa fa-trash-o"></i> Delete </a>
                </td>
              </tr>
              <% } %>
            </tbody>
          </table>
          <!-- end project list -->

        </div>
      </div>
    </div>
  </div>
  <div class="clearfix"></div>


</div>
</div>
</div>
<!-- /page content -->
